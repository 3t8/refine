{
  "name": "@pankod/refine-hasura",
  "description": "refine Hasura data provider. refine is a React-based framework for building internal tools, rapidly. It ships with Ant Design System, an enterprise-level UI toolkit.",
  "version": "3.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch --dts",
    "build": "tsup --dts --format esm,cjs,iife --minify",
    "test": "jest --passWithNoTests --runInBand",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "author": "Pankod",
  "module": "dist/index.mjs",
  "devDependencies": {
    "nock": "^13.1.3",
    "tsup": "^5.11.13",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@pankod/refine-core": "^3.4.0",
    "gql-query-builder": "^3.5.5",
    "graphql": "^15.6.1",
    "graphql-request": "^3.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": false
  }
}