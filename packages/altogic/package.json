{
  "name": "@pankod/refine-altogic",
  "description": "refine Altogic data provider. refine is a React-based framework for building internal tools, rapidly. It ships with Ant Design System, an enterprise-level UI toolkit.",
  "version": "3.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch --dts",
    "build": "tsup --dts --format esm,cjs,iife --minify",
    "test": "jest --passWithNoTests --runInBand",
    "prepare": "npm run build"
  },
  "author": "Pankod",
  "module": "dist/index.mjs",
  "devDependencies": {
    "nock": "^13.1.3",
    "tsup": "^5.11.13",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@pankod/refine-core": "^3.4.0",
    "axios": "^0.21.4",
    "query-string": "^7.0.1"
  },
  "gitHead": "829f5a516f98c06f666d6be3e6e6099c75c07719",
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": false
  }
}