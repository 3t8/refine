{
    "compilerOptions": {
        "module": "esnext",
        "lib": ["dom", "esnext", "DOM.Iterable", "ES6"],
        "importHelpers": true,
        "declaration": true,
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "jsx": "react",
        "esModuleInterop": true,
        "noEmit": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true
    }
}
